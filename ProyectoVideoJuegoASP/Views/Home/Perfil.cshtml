@model ProyectoVideoJuegoASP.Models.USUARIO

<h2>Perfil</h2>


<fieldset class="form-horizontal">
    <legend class="label"> Informacion de Usuario</legend>
        <div class="form-group">
            <div class=""> Nombre</div>
            <div class="text-muted"> @Html.DisplayFor(m => m.nombre) </div>
        </div>
    
        <div class="form-group">
            <div class=""> UserName  </div>
            <div class="text-muted"> @Html.DisplayFor(m => m.username)
            </div>
        </div>
        <div class="form-group">
            <div class=""> Apellido</div>
            <div class="text-muted"> @Html.DisplayFor(m => m.apellido) </div>
        </div>
        <div class="form-group">
            <div class=""> Sexo</div>
            <div class="text-muted"> @Html.DisplayFor(m => m.sexo) </div>
        </div>
        <div class="form-group">
            <div class=""> Email</div>
            <div class="text-muted"> @Html.DisplayFor(m => m.email) </div>
        </div>
@using (@Html.BeginForm("Reporte", "Home", FormMethod.Post, new { @class = "form-horizontal" }))
{
    <div class="form-group">
        <button type="submit" class="btn btn-primary">Reportar</button>
    </div>
}
</fieldset>

